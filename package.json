{
  "name": "jhx",
  "version": "0.0.1",
  "main": "src/js/index.js",
  "scripts": {
    "sass": "sass --style=compressed src/scss/index.scss _site/css/styles.css",
    "dev": "npm-run-all sass --parallel dev:*",
    "dev:parcel": "parcel watch src/js/index.js -d _site/js",
    "dev:11ty": "eleventy --serve --input=src/pages",
    "dev:sass": "npm run sass -- --watch",
    "build": "npm-run-all sass prod:*",
    "build:parcel": "parcel build src/js/index.js -d _site/js",
    "build:11ty": "ELEVENTY_ENV=production eleventy --input=src/pages"
  },
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.38.0"
  }
}